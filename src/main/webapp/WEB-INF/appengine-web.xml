<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>flutter-study-api</application>
    <version>1</version>
    <threadsafe>true</threadsafe>
    <runtime>java8</runtime>
    <service>default</service>
    <sessions-enabled>true</sessions-enabled>
    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/classes/logging.properties"/>
<!--        <property name="cloudsql" value="jdbc:mysql://google/${sql.database}?useSSL=false&amp;cloudSqlInstance=${sql.connection_name}&amp;socketFactory=com.google.cloud.sql.mysql.SocketFactory&amp;user=${sql.user}&amp;password=${sql.password}" />-->
    </system-properties>
    <env-variables>
        <env-var name="SERVICE__NAME" value="FlutterBundangStudy"/>
    </env-variables>
</appengine-web-app>